{
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "endOfLine": "lf",
  "insertPragma": false,
  "jsxSingleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 100,
  "quoteProps": "as-needed",
  "singleQuote": true,
  "semi": true,
  "plugins": ["prettier-plugin-css-order"]
}
